# Supabase Configuration (REQUIRED untuk upload PDF!)
# Get these from your Supabase project dashboard: https://supabase.com/dashboard
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# OpenAI API (Optional - untuk fitur AI chat dan quiz generation)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Google Cloud Vision API (Optional - untuk fitur OCR)
# Create a service account in Google Cloud Console and download the JSON key
GOOGLE_CLIENT_EMAIL=
GOOGLE_PRIVATE_KEY=
GOOGLE_PROJECT_ID=
